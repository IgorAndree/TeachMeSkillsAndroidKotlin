<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/test_container"
        android:layout_width="match_parent"
        android:layout_height="500dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/navigate_to"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:text="@string/test_nav_to_text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/navigate_back"
        app:layout_constraintTop_toBottomOf="@+id/test_container" />

    <Button
        android:id="@+id/navigate_back"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:text="@string/test_nav_back_text"
        app:layout_constraintStart_toEndOf="@+id/navigate_to"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/test_container" />

</androidx.constraintlayout.widget.ConstraintLayout>